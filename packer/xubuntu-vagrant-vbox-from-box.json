{
  "builders": [
    {
      "type": "vagrant",
      "communicator": "ssh",
      "source_path": "./rose-srv6-xubuntu-base.box",
      "box_name" : "rose-beta1",      
      "provider": "virtualbox",
      "add_force": true
    }
  ],
  "provisioners": [
    {
      "type": "file",
      "source": "upload_folder/",
      "destination": "/tmp"    }
    {
      "type": "shell",
      "script": "./start-xubuntu-vagrant-vbox-from-box.sh"
    }
  ]
}


