{
  "builders": [
    {
      "type": "vagrant",
      "communicator": "ssh",
      "source_path": "./rose-srv6-xubuntu-base.box",
      "box_name" : "rose-v2.0",      
      "provider": "virtualbox",
      "add_force": true
    }
  ],
  "provisioners": [
    {
      "type": "file",
      "source": "./execute-xubuntu-vagrant-vbox-from-box.sh",
      "destination": "/tmp/execute-xubuntu-vagrant-vbox-from-box.sh"    
    },
    {
      "type": "shell",
      "script": "./start-xubuntu-vagrant-vbox-from-box.sh"
    }
  ]
}


